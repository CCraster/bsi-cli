#!/usr/bin/env node
const program = require("commander");
program.version(`wk-cli ${ require("../package").version }`)
  .usage("<command> [options]");

program.command("new <app-name>")
  .description("create a new project by wk-cli")
  .action((name, cmd) => {
    console.log(name, cmd);
  });
program.command("compress <file>","compress html css js");
//自定义帮助信息
program.on("--help", function() {
  console.log("  Examples:");
  console.log("");
  console.log("$ app module moduleName");
  console.log("$ app m moduleName");
});
program.parse(process.argv);
